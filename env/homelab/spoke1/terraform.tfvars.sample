# ALL
azure_all_subnet_range_list = ["172.22.0.0/16", ] # Azure 内のサブネット全体レンジリスト

# spoke1
spoke_resource_group   = "spoke1-rg"          # spoke リソースグループ名
spoke_environment_tags = "homelab"            # spoke リソースタグ
spoke_location         = "japaneast"          # spoke ロケーション (japaneast = 東日本リージョン)
spoke_vnet_name        = "spoke1-vnet"        # spoke VNET 名
spoke_address_space    = ["172.22.16.0/20", ] # spoke VNET アドレススペース (List)
spoke_subnet1          = "172.22.16.0/24"     # spoke VM リソース用サブネット

## spoke1 linux vm (Default: Ubuntu20.04 LTS Server)
spoke_vm1_name                    = "spoke1-bastion-vm" # spoke linux vm hostname
spoke_vm1_size                    = "Standard_B1s"      # spoke linux vm size
spoke_vm1_admin_username          = "adminuser"         # spoke linux vm admin username
spoke_vm1_admin_ssh_key_file_path = "./id_rsa.pub"      # spoke linux vm ssh public key file path
spoke_vm1_os_disk_type            = "Standard_LRS"      # spoke linux vm os disk type

